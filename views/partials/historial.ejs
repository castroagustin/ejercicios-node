<h1 class="mb-3 mt-5">Historial</h1>
<% if(historialArr.length >= 1) { %>
<table class="table">
    <thead>
      <tr>
        <th scope="col">Nombre</th>
        <th scope="col">Apellido</th>
        <th scope="col">Edad</th>
      </tr>
    </thead>
    <tbody>
    <% historialArr.forEach(data => { %>
      <tr>
        <td><%= data.name %></td>
        <td><%= data.surname %></td>
        <td><%= data.age %></td>
      </tr>
      <%})%>
    </tbody>
  </table>
<%} else {%>
    <div class="alert alert-danger">
        No se encontraron datos
      </div>
<%}%>